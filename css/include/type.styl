// Typography

h1
  font-size 36px
  line-height 45px
  font-weight 600
  margin 0px 0px 40px

h2
  font-size 30px
  font-weight 600
  margin-bottom 0px

p
  font-size 20px
  font-weight 400
  line-height 30px

a
  text-decoration none

ul
  margin 0px
  padding 0px
  list-style-type none

b, strong, bold
  font-weight 700

.blue
  background-color blue
  color white

.black
  background-color black
  color white